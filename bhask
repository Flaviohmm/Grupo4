#include <iostream>
#include <cstdlib>
#include <cmath>

using namespace std;

int main()
{

float a, b, c, delta;

double  x1, x2, raiz;

cout<<"digite A: ";
cin>>a;

cout<<"digite B: ";
cin>>b;

cout<<"digite C: ";
cin>>c;

delta=((b*b)-4*a*c);
raiz=sqrt(delta);

x1=(-b + raiz)/2*a;

x2=(-b - raiz)/2*a;

cout<<"X1 e': "<<x1;
cout<<"\nX2 e': "<< x2;

cin.get();
}
